{
  "name": "lottie-mvp",
  "version": "1.0.0",
  "description": "",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": "index.html",
  "scripts": {
    "build": "npm run clean && npm run generate && npm run bundle-index && npm run bundle-pages",
    "generate": "npx ts-node build.ts",
    "bundle-index": "npx parcel build index.html --no-source-maps",
    "bundle-pages": "cd build && npx parcel build *.html --no-source-maps",
    "serve": "cd dist && npx http-server -p 8080 -a 127.0.0.1 -c -1",
    "lint": "npx eslint *.ts src/*.ts",
    "clean": "rm -rf ./dist/* ./build/*",
    "check": "npx tsc --noEmit *.ts src/*.ts",
    "test": "echo ${PWD}"
  },
  "author": "",
  "license": "Apache 2.0",
  "dependencies": {
    "handlebars": "^4.7.7"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.3.2",
    "@parcel/transformer-typescript-tsc": "^2.3.2",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "http-server": "^14.1.0",
    "parcel": "^2.3.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
